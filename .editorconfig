resharper_braces_for_ifelse = required_for_multiline_statement
braces_for_ifelse = required_for_multiline_statement

resharper_csharp_braces_for_while = required_for_multiline_statement
csharp_braces_for_while = required_for_multiline_statement
resharper_braces_for_while = required_for_multiline_statement
braces_for_while = required_for_multiline_statement

