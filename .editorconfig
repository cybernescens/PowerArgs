[*]
indent_style = space
indent_size = 4
tab_width = 2

[*.cs]
blank_lines_around_single_line_property = 0
csharp_blank_lines_around_single_line_property = 0

blank_lines_around_auto_property = 0
resharper_blank_lines_around_auto_property = 0

resharper_csharp_braces_for_ifelse = required_for_multiline_statement
